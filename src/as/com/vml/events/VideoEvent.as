package  com.vml.events{	import flash.events.Event;	public class VideoEvent extends Event		{				public static const VIDEO_COMPLETE:String = "videoComplete";		public static const VIDEO_START:String = 'videoStart';			public static const VIDEO_DOWNLOAD_PERCENT:String = 'videoDownloadPercent';		public static const VIDEO_PLAY_PERCENT:String = 'videoPlayPercent';		public static const VIDEO_BUFFERING:String = 'videoBuffering';		public static const VIDEO_BUFFER_FULL:String = 'videoBufferFull';			// FROM UI CONTROLLER TO PLAYER 		public static const MUTE_VIDEO:String = "muteVideo";		public static const UNMUTE_VIDEO:String = "unmuteVideo";		public static const PLAY_VIDEO:String = 'playVideo';		public static const PAUSE_VIDEO:String = 'pauseVideo';		public static const SEEK_VIDEO:String = 'seekVideo';		public static const RESTART_VIDEO:String = "videoRestart";		public static const SCRUB_SOUND:String = "scrubSound";		public static const SKIP:String = "skip";						public static const	VIDEO_CONNECTION_ERROR:String = 'videoConnectionError';		public static const	VIDEO_CONNECTION_READY:String = 'videoConnectionReady';				public static const	VIDEO_TIME:String = 'videoTime';				public var data:Object;				public function VideoEvent(type:String, bubbles:Boolean = false, data:Object = null)		{			super(type, bubbles);			this.data = data;		}					override public function clone():Event 		{			return new VideoEvent( this.type, this.bubbles, this.data ) as Event;		}	}}